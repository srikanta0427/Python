   "source": [
    "class Points:\n",
    "    def __init__(self,x,y):\n",
    "        self.x=x\n",
    "        self.y=y\n",
    "    def __add__(self,other):\n",
    "        return Points(self.x + other.x, self.y+other.y)\n",
    "    def __str__(self):\n",
    "        return f\"{self.x}, {self.y}\"\n"
   ]
